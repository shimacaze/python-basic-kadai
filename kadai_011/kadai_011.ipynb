        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "count = 0\n",
        "index = 0\n",
        "while count < 2:\n",
        "  print(array[index])\n",
        "  index += 1\n",
        "\n",
        "  if index == len(array):\n",
        "    index = 0\n",
        "    count += 1"
